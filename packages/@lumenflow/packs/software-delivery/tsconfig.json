{
  "extends": "../../../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2023",
    "lib": ["ES2023"],
    "outDir": "./dist",
    "rootDir": ".",
    "noEmit": false,
    "declaration": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowJs": false,
    "checkJs": false,
    "noEmitOnError": true
  },
  "include": ["*.ts", "tools/**/*.ts", "tool-impl/**/*.ts"],
  "exclude": ["node_modules", "dist", "__tests__"]
}
