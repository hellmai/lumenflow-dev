{
  "name": "@lumenflow/metrics",
  "version": "3.6.1",
  "description": "DORA/SPACE analytics and flow metrics for LumenFlow workflow framework",
  "keywords": [
    "lumenflow",
    "metrics",
    "dora",
    "space",
    "devops",
    "analytics",
    "workflow"
  ],
  "homepage": "https://github.com/hellmai/lumenflow-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/hellmai/lumenflow-dev.git",
    "directory": "packages/@lumenflow/metrics"
  },
  "license": "AGPL-3.0-only",
  "author": {
    "name": "HellmAI",
    "url": "https://hellm.ai"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./telemetry": {
      "types": "./dist/telemetry/index.d.ts",
      "import": "./dist/telemetry/index.js"
    },
    "./dora": {
      "types": "./dist/dora/index.d.ts",
      "import": "./dist/dora/index.js"
    },
    "./flow": {
      "types": "./dist/flow/index.d.ts",
      "import": "./dist/flow/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:dist": "tsc -p tsconfig.build.json",
    "pack:dist": "pnpm pack",
    "clean": "rm -rf dist *.tgz",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "simple-statistics": "^7.8.8"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  }
}
