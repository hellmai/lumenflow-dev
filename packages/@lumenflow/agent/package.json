{
  "name": "@lumenflow/agent",
  "version": "0.0.0",
  "description": "Agent session management for LumenFlow workflow framework",
  "keywords": [
    "lumenflow",
    "workflow",
    "agent",
    "session",
    "logging"
  ],
  "homepage": "https://github.com/hellmai/os",
  "repository": {
    "type": "git",
    "url": "https://github.com/hellmai/os.git",
    "directory": "packages/@lumenflow/agent"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "HellmAI",
    "url": "https://hellm.ai"
  },
  "type": "module",
  "exports": {
